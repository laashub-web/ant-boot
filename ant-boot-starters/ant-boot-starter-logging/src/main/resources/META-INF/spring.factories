# Application listeners
org.springframework.context.ApplicationListener=\
  org.antframework.boot.logging.boot.AntLoggingApplicationListener
# Log initializers
org.antframework.boot.logging.LogInitializer=\
  org.antframework.boot.logging.initializer.LevelInitializer,\
  org.antframework.boot.logging.initializer.ConsoleAppenderInitializer,\
  org.antframework.boot.logging.initializer.FileAppenderInitializer,\
  org.antframework.boot.logging.initializer.ErrorFileAppenderInitializer
